<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Welcome to LogSynq </h1>
    <% files.forEach(file => { %>
        <ul><%= file.dir.name %> / 
            <% file.files.forEach(f => { %>
                <li>
                    <a href=<%= file.dir.path + ""+ file.dir.name + "/" +f %>> <%= f %>  </a>
                </li>
            <% }) %>
        </ul>
    <% }) %>
</body>
</html>